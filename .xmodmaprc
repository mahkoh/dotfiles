clear lock
clear control
clear shift
clear mod4

! caps_lock -> super_r -> ctrl
keycode 66 = Super_R
add Control = Super_R

! esc = caps_lock
keycode 9 = Caps_Lock

! menu -> third level
keycode 135 = ISO_Level3_Shift

! left extra key -> esc, dollar
keycode 94 = Escape dollar

! altgr -> shift
keycode 108 = Shift_L

! print -> home
keycode 107 = Home

! pause -> end
keycode 127 = End

! umlaute
keysym a = a A a A adiaeresis Adiaeresis
keysym o = o O o O odiaeresis Odiaeresis
keysym u = u U u U udiaeresis Udiaeresis
keysym s = s S s S ssharp section

