clear lock
clear control
clear shift
clear mod4

! caps_lock -> super_r -> ctrl
keycode 66 = Super_R
add Control = Super_R

! esc = caps_lock
keycode 9 = Caps_Lock

! menu -> third level
keycode 135 = Mode_switch

! left shift -> third level
keycode 50 = Mode_switch

! left extra key -> esc, dollar
keycode 94 = Escape dollar

! altgr -> shift
keycode 108 = Shift_L

! print -> home
keycode 107 = Home

! pause -> end
keycode 127 = End

! umlaute
keysym a = a A adiaeresis Adiaeresis
keysym o = o O odiaeresis Odiaeresis
keysym u = u U udiaeresis Udiaeresis
keysym s = s S ssharp section

! numpad
keysym b = b B 0
keysym m = m M 1
keysym w = w W 2
keysym v = v V 3
keysym h = h H 4
keysym t = t T 5
keysym n = n N 6
keysym g = g G 7
keysym c = c C 8
keysym r = r R 9

